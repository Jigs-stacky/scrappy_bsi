<?xml version="1.0" encoding="utf-8"?>
<odoo>

     <record id="seq_scraper_log" model="ir.sequence">
        <field name="name">Scraper Log</field>
        <field name="code">scraper.log</field>
        <field name="prefix">LOG-</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

	<record id="seq_scraped_store_data" model="ir.sequence">
        <field name="name">Scraped Store Data</field>
        <field name="code">scraped.store.data</field>
        <field name="prefix">COMP-</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_scraper_request" model="ir.sequence">
        <field name="name">Scraper Request</field>
        <field name="code">scraper.request</field>
        <field name="prefix">REQ/%(year)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>
